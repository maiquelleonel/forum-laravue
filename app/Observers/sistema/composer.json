{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/docode-web/pagseguro"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.2.*",
        "laravelcollective/html": "5.2.*",
        "khill/lavacharts": "^3.0",
        "davejamesmiller/laravel-breadcrumbs": "^3.0",
        "pingpong/menus": "^2.1",
        "barryvdh/laravel-elfinder": "^0.3.10",
        "dlimars/credit-card-returns": "^1.0",
        "laracasts/presenter"   : "^0.2.1",
        "mundipagg/mundipagg-one-php": "dev-master",
        "softr/asaas-php-sdk"   : "1.*",
        "phpsc/pagseguro"       : "dev-master",
        "guzzlehttp/guzzle"     : "^6.1",
        "prettus/l5-repository" : "^2.1",
        "cagartner/correios-consulta": "^0.1",
        "artesaos/defender": "^0.6.2",
        "owen-it/laravel-auditing": "^4.0",
        "mautic/api-library": "^2.6",
        "plivo/plivo-php": "^1.1",
        "rorteg/blingsdk": "dev-master",
        "google/recaptcha": "^1.1",
        "arcanedev/log-viewer": "^4.3",
        "canducci/cep": "^2.0",
        "michael/laravelpagseguro": "^1.0",
        "barryvdh/laravel-debugbar": "^2.4",
        "dlimars/laravel-searchable": "^1.1",
        "league/csv": "^8.2@dev",
        "darkaonline/l5-swagger": "~3.0",
        "netshell/paypal": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "xethron/migrations-generator": "^2.0",
        "symfony/dom-crawler": "~3.0",
        "symfony/css-selector": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev"
}
